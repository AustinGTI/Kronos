{
  "expo": {
    "name": "Kronos",
    "slug": "kronos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kronos_logo_v2.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/images/kronos_icon_v2.png",
      "color": "#e3c9f2"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/kronos_icon_v2.png",
          "color": "#e3c9f2",
          "sounds": [
            "./assets/sounds/on_segment_complete.wav",
            "./assets/sounds/on_session_complete.wav"
          ]
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/kronos_splash_screen_v2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kronos_logo_v2.png",
        "backgroundColor": "#F0D2FF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.aught.Kronos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "49d55d06-2209-4ea2-93be-baf365f421b9"
      }
    },
    "owner": "aught"
  }
}
